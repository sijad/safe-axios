{
  "name": "safe-axios",
  "version": "0.0.1",
  "description": "a drop replacement to prevent ssrf and toctou requests",
  "main": "main/index.js",
  "types": "main/index.d.ts",
  "scripts": {
    "test": "",
    "lint": "eslint --fix 'src/**/*.ts'",
    "typings-check": "tsc --noEmit",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sijad/safe-axios"
  },
  "author": "Sajjad Hashemian",
  "license": "MIT",
  "dependencies": {
    "ip-range-check": "^0.2.0",
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typings-check && yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "yarn run lint",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "yarn run lint",
      "git add"
    ]
  }
}
